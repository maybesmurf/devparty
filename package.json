{
  "name": "devparty",
  "version": "0.0.1-alpha",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "build": "yarn generate && next build",
    "start": "next start -p ${PORT-3000}",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "clean:codegen": "trash \"./src/**/__generated__/**\"",
    "generate": "yarn codegen && prisma generate",
    "migrate": "prisma migrate dev",
    "codegen": "yarn clean:codegen && run-s -l \"codegen:*\"",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register ./src/graphql/index.ts",
    "codegen:graphql": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.4.4",
    "@giraphql/core": "^2.12.0",
    "@giraphql/plugin-scope-auth": "^2.7.1",
    "@giraphql/plugin-simple-objects": "^2.5.1",
    "@giraphql/plugin-validation": "^2.7.1",
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "@hookform/resolvers": "^2.7.0",
    "@prisma/client": "2.28.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.23.0",
    "graphql": "^15.5.1",
    "graphql-helix": "^1.6.1",
    "hash-wasm": "^4.9.0",
    "linkifyjs": "^3.0.0-beta.3",
    "next": "^11.0.2-canary.25",
    "next-iron-session": "^4.2.0",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.15",
    "preact": "^10.5.14",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.12.2",
    "zod": "^3.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/near-operation-file-preset": "^2.0.0",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/linkifyjs": "^2.1.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-unused-imports": "^1.1.2",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.5",
    "prettier": "^2.3.2",
    "prisma": "2.28.0",
    "tailwindcss": "^2.2.4",
    "trash-cli": "^4.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
