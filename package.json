{
  "name": "devparty",
  "private": true,
  "scripts": {
    "dev": "yarn generate && next dev",
    "build": "yarn generate && next build",
    "start": "next start -H 0.0.0.0 -p ${PORT-8080}",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"./src/**/*.{ts,tsx}\"",
    "generate": "prisma generate && yarn codegen",
    "prisma:migrate": "prisma migrate dev --preview-feature",
    "prisma:seed": "ts-node prisma/seed.ts",
    "codegen": "run-s -l \"codegen:*\"",
    "codegen:schema": "ts-node --transpile-only -r tsconfig-paths/register ./src/graphql/index.ts",
    "codegen:graphql": "graphql-codegen",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.4.10",
    "@changesets/cli": "^2.17.0",
    "@giraphql/core": "^2.15.1",
    "@giraphql/plugin-prisma": "^0.9.1",
    "@giraphql/plugin-relay": "^2.15.1",
    "@giraphql/plugin-scope-auth": "^2.10.1",
    "@giraphql/plugin-simple-objects": "^2.8.1",
    "@giraphql/plugin-validation": "^2.10.1",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@hookform/resolvers": "^2.8.0",
    "@prisma/client": "3.0.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.3",
    "graphql-helix": "^1.7.0",
    "hash-wasm": "^4.9.0",
    "ipfs-http-client": "^52.0.3",
    "linkifyjs": "^3.0.0-beta.3",
    "markdown-to-jsx": "^7.1.3",
    "next": "canary",
    "next-iron-session": "^4.2.0",
    "next-seo": "^4.26.0",
    "next-themes": "^0.0.15",
    "nprogress": "^0.2.0",
    "react": "latest",
    "react-cool-inview": "^2.0.7",
    "react-dom": "latest",
    "react-hook-form": "^7.15.2",
    "react-hot-toast": "^2.1.1",
    "spotify-web-api-node": "^5.0.2",
    "timeago.js": "^4.0.2",
    "zod": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.0",
    "@graphql-codegen/near-operation-file-preset": "^2.1.4",
    "@graphql-codegen/typescript": "^2.2.1",
    "@graphql-codegen/typescript-operations": "^2.1.4",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "@types/faker": "^5.5.8",
    "@types/linkifyjs": "^2.1.4",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-unused-imports": "^1.1.4",
    "faker": "^5.5.3",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.4.0",
    "prisma": "3.0.2",
    "tailwindcss": "^2.2.15",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
